import base64
from pprint import pprint
import json
import requests

from base64 import b64encode

client_id = '8a085352819267f'

headers = {"Authorization": "Client-ID 8a085352819267f"}

api_key = 'my-api-key'

url = "https://api.imgur.com/3/upload.json"

j1 = requests.post(
    url, 
    headers = headers,
    data = {
        'key': api_key, 
        'image': b64encode(open('1.jpg', 'rb').read()),
        'type': 'base64',
        'name': '1.jpg',
        'title': 'Picture no. 1'
    }

#turn the returned json into a python dict
j = json.loads(j1.text)
 
#print it out cleanly
pprint(j) 
)